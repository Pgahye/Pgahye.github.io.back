      &lt;td style=&quot;text-align: center&quot;&gt;secure_url&lt;/td&gt;
      &lt;td style=&quot;text-align: center&quot;&gt; &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td style=&quot;text-align: center&quot;&gt;public_id&lt;/td&gt;
